#include<stdio.h>
#include<stdlib.h>
//#include"maxSeq.c"
size_t maxSeq(int*, size_t);

int main()
{
  int array1[] = {};
  int array2[] = {12, 0};
  int array3[] = {-1, 0, 1, 6, 8};
  int array4[] = {1, 99, 2, 5, 6, 98};
  if(maxSeq(array1,0)!= 0){printf("array1\n"); return EXIT_FAILURE;}
  if(maxSeq(array2,2)!= 1){printf("array2\n"); return EXIT_FAILURE;}
  if(maxSeq(array3,5)!= 5){printf("array3\n"); return EXIT_FAILURE;}
  if(maxSeq(array4,6)!= 4){printf("array4\n"); return EXIT_FAILURE;}
  return EXIT_SUCCESS;
}
